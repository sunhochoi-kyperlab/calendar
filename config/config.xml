<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="http://www.obigo.com/ces2020/OBIGO/C300/calendar" version="1.0.3" viewmodes="fullscreen">
    <name xml:lang="en-us" short="Calendar">Calendar</name>
    <description xml:lang="en-us">Seamlessly connections with Google Calendar. You can check your schedule and get reminder in the car.</description>
    <icon src="icon.png"/>
    <thumbnail src="thumbnail.png" />
    <author>Obigo</author>
    <license>Obigo</license>
    <meta-data>
        <manifest-version>1.3.1.0</manifest-version>
        <driving-restriction content="forbidden:close"></driving-restriction>
    </meta-data>
    <content src="index.html" type="text/html"/>
    <access origin="*"/>
    <feature name="http://www.obigo.com/api/applicationmanager.getownerapplication" required="true">
        <param name="version" value="3.1.5.7"/>
    </feature>
    <feature name="http://www.obigo.com/api/storage" required="true">
    <param name="version" value="1.1.0.0"/>
    </feature>
    <feature name="http://www.obigo.com/api/SVG" required="true">
        <param name="version" value="1.1.0.0"/>
    </feature>
    <feature name="http://www.obigo.com/api/network" required="true">
      <param name="version" value="1.1.0.0"/>
    </feature>
</widget>
